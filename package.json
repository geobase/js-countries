{
  "name": "smart-geo",
  "version": "0.1.8",
  "description": "Smart Geo is data from Open Data providers compiled into easy to use JavaScript objects",
  "scripts": {
    "data-update": "babel-node --eval \"require('./bin/data-update')().catch(err => console.error(err.stack))\"",
    "build": "webpack -p",
    "test": "mocha --compilers js:babel/register --recursive"
  },
  "main": "./build/smart-geo.js",
  "keywords": [
    "geo",
    "country",
    "countries",
    "region",
    "regions",
    "province",
    "provinces",
    "state",
    "states"
  ],
  "author": "Gabriel Bull",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/smart-io/js-geo"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "chai": "^3.3.0",
    "eslint": "^1.5.1",
    "eslint-loader": "^1.0.0",
    "json-loader": "^0.5.3",
    "mkdirp": "^0.5.1",
    "mocha": "^2.2.5",
    "request": "^2.63.0",
    "temp": "^0.8.3",
    "webpack": "^1.12.2"
  }
}
